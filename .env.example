# Example environment variables for agent_search_product

# Qdrant
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=

# Redis (optional)
REDIS_URL=redis://localhost:6379/0

# Embeddings provider (para vectores)
EMBEDDINGS_PROVIDER=ollama  # Opciones: ollama, gemini, openai

# LLM provider (para clasificaci√≥n, prompts, chat, etc)
LLM_PROVIDER=gemini         # Opciones: gemini, openai, ollama

# Gemini
GOOGLE_API_KEY=your-google-api-key

# OpenAI
OPENAI_API_KEY=sk-xxxx

# Ollama
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3

# Model name for LLM (optional, for reference)
MODEL_NAME=gemini-2.5-flash
